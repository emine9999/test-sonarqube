pipeline {
    agent any
    
    environment {
        SONARQUBE_URL = "http://128.110.217.203:9000"
        SONARQUBE_TOKEN = "b93391a0b487e82c143aa36c3955f16a6aa655ad"
    }
    
    stages {
        stage('Debug') {
            steps {
                script {
                    echo "SONARQUBE_URL: ${SONARQUBE_URL}"
                    echo "SONARQUBE_TOKEN: ${SONARQUBE_TOKEN}"
                    sh "curl -X GET ${SONARQUBE_URL}/api/system/status"
                }
            }
        }
        
        stage('SonarQube Analysis') {
            steps {
                script {
                    def scannerHome = tool 'sonarqube-scanner'
                    withSonarQubeEnv('SonarQube') {
                        sh """
                            ${scannerHome}/bin/sonar-scanner \
                                -Dsonar.projectKey=my-js-sample-project \
                                -Dsonar.sources=. \
                                -Dsonar.host.url=${SONARQUBE_URL} \
                                -Dsonar.login=${SONARQUBE_TOKEN} \
                                -Dsonar.java.additionalOpts="--add-opens java.base/java.lang=ALL-UNNAMED"
                        """
                    }
                }
            }
        }
        
        stage('Build') {
            steps {
                echo 'Building the project...'
            }
        }
    }
    
    post {
        always {
            echo 'Cleaning up...'
        }
    }
}
